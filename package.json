    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-transform-runtime": "7.4.3",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "7.4.0",
    "@babel/runtime": "7.4.3",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-plugin-import": "1.10.0",
    "babel-plugin-lodash": "3.3.4",
